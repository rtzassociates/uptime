<%= render :partial => "server_details", :locals => { :server => @database_server } %>

<h4>Databases Hosted</h4>
<table class="table">
  <tr>
    <th>Database Name</th>
  </tr>
  <% @database_server.databases.each do |database| %>
    <tr>
      <td><%= database.name %></td>
    </tr>
  <% end %>
</table>